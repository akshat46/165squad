<div>
	<div class="modal-header">
    	<h3 class="modal-title text-center text-danger"><% dialogMsg %></h3>
	</div>
	 <div class="container-fluid">
	  <div class="row">
	   <div class="col-md-12">
	    <div class="modal-footer">
	    	<div class="footer-buttons" style="">
	            <button style="float:left;"class="btn btn-danger btn-sm" type="button" ng-click="ok()">Yes</button>
	            <button style="float:left;" class="btn btn-warning btn-sm" type="button" ng-click="cancel()">Cancel</button>
            </div>
		</div>
	   </div>
	  </div>
	 </div>
</div>